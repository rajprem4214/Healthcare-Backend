{
  "name": "uwell-backend",
  "version": "0.0.1",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "dev-old": "tsc && nodemon src/index.ts",
    "start": "node dist/src/index.js",
    "build": "npm run clean && tsc --project tsconfig.build.json && npm run sentry:sourcemaps && npm run sentry:sourcemaps",
    "dev": "ts-node-dev --poll --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:check": "drizzle-kit check:mysql",
    "migrate": "drizzle-kit generate:mysql && ts-node src/db/migrate.ts",
    "migrate:create": "drizzle-kit generate:mysql",
    "migrate:push": "ts-node src/db/migrate.ts",
    "migrate:drop": "drizzle-kit drop",
    "db:introspect": "drizzle-kit introspect:mysql",
    "db:push": "drizzle-kit push:mysql",
    "db:update-drizzle": "drizzle-kit up:mysql",
    "db:dashboard": "drizzle-kit studio --port 3010 --verbose",
    "postinstall": "npx typesync",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org inuversal-group --project inuwell-backend-dev ./dist && sentry-cli sourcemaps upload --org inuversal-group --project inuwell-backend-dev ./dist"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.460.0",
    "@aws-sdk/s3-request-presigner": "^3.460.0",
    "@medplum/core": "^2.1.10",
    "@medplum/fhirtypes": "^2.1.14",
    "@novu/node": "^0.22.0",
    "@planetscale/database": "^1.11.0",
    "@sentry/cli": "^2.26.0",
    "@sentry/node": "^7.92.0",
    "@sentry/profiling-node": "^1.3.3",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.1.4",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.44.7",
    "@types/express": "^4.17.17",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemon": "^1.19.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "drizzle-kit": "^0.20.13",
    "esbuild": "^0.19.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "<5.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}